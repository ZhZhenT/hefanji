<scroll-view scroll-y style="height:{{contentheight}}px; position:reative;" lower-threshold='50' bindscrolltolower='bindLoadScrollTolower'>




  <view class='progress {{progressShow?"show":""}}'></view>

  <block wx:if='{{items.isshow}}' wx:key='{{index}}' wx:for='{{ordergoodslist1}}' wx:for-item="items">
    <view class='top'>
      <text class='ads'>{{items.container.address}}</text>
    </view>
    <block wx:for='{{items.detail}}' wx:key='{{index}}'>

      <view data-orderID='{{items.id}}' catchtap='bindToOrderNewDetail'>
            <view class='order-id-date'>
        <view>订单编号：{{items.id}}</view>
        <view>下单时间：{{items.created_at}}</view>
      </view>

      <view class='new-goods-wrap'>
        <view wx:key='{{index}}' class='goods ' wx:for='{{item.products}}' wx:for-item="val">
          <view data-goodsId='{{val.product.id}}' class='img'>
            <image src='{{val.product.feature_image}}' style='width:95rpx;height:95rpx;display:block'>
            </image>
          </view>
        </view>
      </view>

      <view class='order-zf-state'>
        <view>实际支付：
          <text class='money'>￥{{items.amount}}</text> </view>
        <view  class='btn col8f8f8f' style='position:absolute;right:170rpx' data-orderID='{{items.id}}' bindtap='bindToOrderNewDetail'>申请退货</view>
        <view wx:if='{{!items.is_ended}}' class='btn' data-orderID='{{items.id}}' catchtap='bindToOrderNewDetail'>去取餐</view>
        <view wx:if='{{items.is_ended}}' data-orderID='{{items.id}}' catchtap='bindToOrderNewDetail' class='btn col8f8f8f'>已结束</view>
      </view>

      <view class='order-line'>

      </view>
      </view>

    </block>

    <!-- <view class='order-info-box'>
      <view class='order-info'>
        <view class='left'>
          <view>订单编号：{{items.id}}</view>
          <view>下单时间：{{items.created_at}}</view>
          <view>温馨提示：每日取餐时间为 {{promptlayerTxT}}</view>
        </view>
        <view class='right'>
          <view class='goods-price'>￥{{items.amount}}
            <view wx:if='{{items.oldprice && items.amount!=items.oldprice}}' class='goods-old-price'>￥{{items.oldprice}}</view>
          </view>
          <view class='txt'>已经支付</view>
        </view>
      </view>
    </view> -->

  </block>


  <view wx:if='{{showloading}}' class='loading'>{{loadMoreData}}</view>


  <view wx:if='{{showorderno}}' class='ordernobox'>
    <view class='icon'>亲！您还没有任何订单哦！</view>
  </view>


</scroll-view>

<tabBarCustom show='{{show}}' class='botmenu' current="1" style="position: absolute;bottom: 0;left:0;right:0;"></tabBarCustom>